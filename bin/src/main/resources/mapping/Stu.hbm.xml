<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="top.zzh.bean.Stu" table="t_stu" schema="d_ssh_case">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="num" column="num"/>
        <property name="realName" column="real_name"/>

        <one-to-one name="stuDetail" class="top.zzh.bean.StuDetail" />

        <many-to-one name="clazz" class="top.zzh.bean.Clazz" column="class_id"/>

        <set name="contractSet" table="t_stu_contract">
            <key column="stu_id"/>
            <many-to-many class="top.zzh.bean.Contract" column="contract_id" />
        </set>

    </class>
</hibernate-mapping>